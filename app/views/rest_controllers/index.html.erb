<p id="notice"><%= notice %></p>

<h1>Listing Rest Controllers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rest_controllers.each do |rest_controller| %>
      <tr>
        <td><%= link_to 'Show', rest_controller %></td>
        <td><%= link_to 'Edit', edit_rest_controller_path(rest_controller) %></td>
        <td><%= link_to 'Destroy', rest_controller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rest controller', new_rest_controller_path %>
