<p id="notice"><%= notice %></p>

<h1>Listing Associated Trials</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @associated_trials.each do |associated_trial| %>
      <tr>
        <td><%= link_to 'Show', associated_trial %></td>
        <td><%= link_to 'Edit', edit_associated_trial_path(associated_trial) %></td>
        <td><%= link_to 'Destroy', associated_trial, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Associated trial', new_associated_trial_path %>
